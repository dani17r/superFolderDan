{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/app/components/*"],
      "@layouts/*": ["src/app/layouts/*"],
      "@pages/*": ["src/app/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@stores/*": ["src/stores/*"],
      "@router/*": ["src/router/*"],
      "@utils/*": ["src/utils/*"],
      "@boot/*": ["src/boot/*"],
      "@app/*": ["src/app/*"],
      "@src/*": ["src/*"]
    }
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}
